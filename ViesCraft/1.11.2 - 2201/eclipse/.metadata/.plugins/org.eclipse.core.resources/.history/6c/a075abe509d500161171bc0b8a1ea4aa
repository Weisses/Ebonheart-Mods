		//V4
		item_airship_v4_wood0 = registerItem(new ItemAirshipV4Wood0("v4/wood0/item_airship_v4"));
		item_airship_v4_iron = registerItem(new ItemAirshipV4Iron("v4/iron/item_airship_v4"));
