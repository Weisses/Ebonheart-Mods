		item_airship_v2_gold = registerItem(new ItemAirshipV2Gold("v2/gold/item_airship_v2"));
		item_airship_v2_lapislazuli = registerItem(new ItemAirshipV2LapisLazuli("v2/lapislazuli/item_airship_v2"));
